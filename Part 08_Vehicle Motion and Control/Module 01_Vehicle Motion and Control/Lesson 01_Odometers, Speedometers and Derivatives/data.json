        {
          "id": 466904,
          "key": "1d765c65-5843-4114-a49c-5cf511fc2516",
          "title": "Inertial Navigation Sensors",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "1d765c65-5843-4114-a49c-5cf511fc2516",
