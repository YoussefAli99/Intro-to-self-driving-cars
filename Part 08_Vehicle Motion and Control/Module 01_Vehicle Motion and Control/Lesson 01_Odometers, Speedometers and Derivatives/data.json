            "last_viewed_at": null,
            "unstructured": null
          },
          "resources": null,
          "atoms": [
            {
              "id": 486250,
              "key": "98ab787d-5bb9-4d52-9968-ddbd5b6faafb",
              "title": "Delta X Over Delta T",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "AsBTLDf0vQs",
                "china_cdn_id": "AsBTLDf0vQs.mp4"
              }
            },
            {
              "id": 475768,
              "key": "48218a76-a6a8-430b-a5e3-ac44e5a31e06",
              "title": null,
              "semantic_type": "TextAtom",
              "is_public": true,
              "text": "### Solution steps\n<div class=\"mathquill\">\nv_{\\text{avg}} = \\frac{\\text{distance traveled}}{\\text{time elapsed}}\n</div>\n\n<div class=\"mathquill\">\nv_{\\text{avg}} = \\frac{\\Delta x}{\\Delta t}\n</div>\n\n<div class=\"mathquill\">\nv_{\\text{avg}} = \\frac{x_{\\text{final}} - x_{\\text{initial}}}{t_{\\text{final}} - t_{\\text{initial}}}\n</div>\n\n<div class=\"mathquill\">\nv_{\\text{avg}} = \\frac{80-30}{3-2}\n</div>\n\n<div class=\"mathquill\">\nv_{\\text{avg}} = 50 \\text{mph}\n</div>",
              "instructor_notes": ""
            }
          ]
        },
