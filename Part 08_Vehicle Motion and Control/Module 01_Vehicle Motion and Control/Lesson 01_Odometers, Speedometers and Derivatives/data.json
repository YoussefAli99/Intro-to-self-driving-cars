          "resources": null,
          "atoms": [
            {
              "id": 485990,
              "key": "fc46cf7f-039b-4223-9616-e9040688ab27",
              "title": "L1 04 B V2",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "jR7_6Zqq228",
                "china_cdn_id": "jR7_6Zqq228.mp4"
