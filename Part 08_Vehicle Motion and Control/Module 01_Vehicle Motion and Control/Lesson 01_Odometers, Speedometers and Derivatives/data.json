              "url": "https://video.udacity-data.com/topher/2017/December/5a3421c7_example-trajectory/example-trajectory.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/0f8a407a-172e-4d71-ba23-c8ab5910c180",
