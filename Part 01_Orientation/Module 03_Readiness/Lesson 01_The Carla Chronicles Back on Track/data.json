              "is_public": true,
              "url": "https://video.udacity-data.com/topher/2017/September/59be1585_udacity-challenge-shades-3/udacity-challenge-shades-3.jpg",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/036324bf-74f0-4918-b564-d0eaf284e894",
